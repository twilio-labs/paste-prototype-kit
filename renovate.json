{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", "group:allNonMajor", "schedule:earlyMondays"],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "rangeStrategy": "update-lockfile"
    },
    {
      "matchPackageNames": ["react", "react-dom"],
      "enabled": false
    }
  ]
}
